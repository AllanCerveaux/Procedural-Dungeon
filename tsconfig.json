{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "composite": true,
    "resolveJsonModule": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "downlevelIteration": true,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "typeRoots": ["node_modules/phaser/types/", "./types/"],
    "lib": ["es2018", "dom", "esnext.asynciterable"],
    "paths": {}
  }
}
